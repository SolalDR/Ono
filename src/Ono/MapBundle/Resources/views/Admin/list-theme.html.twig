
{% extends "OnoMapBundle:Admin:layout.html.twig" %}

{% block body %}

<div class="mainContent">
  <h1>Liste des thèmes</h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>
          Id
        </th>
        <th>
          Nom du thèmes
        </th>
        <th>
          Description
        </th>
      </tr>
    </thead>
    <tbody>
      {% for theme in themes %}
      <tr>
        <td>
          {{ theme.id }}
        </td>
        <td>
          {{ theme.libTheme }}
        </td>
        <td>
          {{ theme.description }}
        </td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
</div>

{% endblock %}
